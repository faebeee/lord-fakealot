{
    "imports": [
        "./controllers.json"
    ],
    "parameters": {
        "fakerseed": 123,
        "port": 1337,
        "host": "localhost"
    },
    "services": {
        "service.server": {
            "file": "../src/Server.js",
            "arguments": [
                "%port%",
                "%host%",
                "@service.logger"
            ]
        },
        "service.filegenerator": {
            "file": "../src/FileGenerator.js",
            "arguments": [
                "%interfaceSourceDir%",
                "@service.tsschemaloader",
                "@service.tsdatagenerator",
                "@service.schemadatawriter",
                "@service.logger"
            ]
        },
        "service.tsschemaloader": {
            "file": "../src/SchemaLoader.js",
            "arguments": [
                "%interfaceSourceDir%",
                "@service.logger",
                "%tsconfig%"
            ]
        },
        "service.tsdatagenerator": {
            "file": "../src/DataGenerator.js",
            "arguments": [
                "%fakerseed%"
            ]
        },
        "service.schemadatawriter": {
            "file": "../src/DataFileWriter.js"
        },
        "service.logger": {
            "file": "../src/Logger.js",
            "arguments": [
                "%loglevel%"
            ]
        }
    }
}
