{
    "imports": [
        "./controllers.json"
    ],
    "parameters": {
        "fakerseed": 123,
        "port": 1337,
        "host": "localhost"
    },
    "services": {
        "service.server": {
            "file": "../src/Server.js",
            "arguments": [
                "%port%",
                "%host%"
            ]
        },
        "service.datafilegenerator": {
            "file": "../src/DataFileGenerator.js",
            "arguments": [
                "%interfaceSourceDir%",
                "@service.tsschemaloader",
                "@service.tsdatagenerator",
                "@service.schemadatawriter"
            ]
        },
        "service.tsschemaloader": {
            "file": "../src/typescript/SchemaLoader.js",
            "arguments": [
                "%interfaceSourceDir%"
            ]
        },
        "service.tsdatagenerator": {
            "file": "../src/typescript/DataGenerator.js",
            "arguments": [
                "%fakerseed%",
                "%fakerlocale%"
            ]
        },
        "service.schemadatawriter": {
            "file": "../src/SchemaDataWriter.js"
        }
    }
}
